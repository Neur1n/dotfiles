# vim: ft=sh

export GZ_WS=/home/yy/Documents/ws/gazebo_ws

alias cdgz="cd $GZ_WS"
alias gzv="gazebo -u --verbose"

source /usr/share/gazebo/setup.sh
export GAZEBO_LIBRARY_DIRS=/usr/include/gazebo-8
export GAZEBO_MODEL_PATH=${GAZEBO_MODEL_PATH}:${GZ_WS}/models
export GAZEBO_PLUGIN_PATH=${GAZEBO_PLUGIN_PATH}:${GZ_WS}/lib
export GAZEBO_RESOURCE_PATH=${GAZEBO_RESOURCE_PATH}:${GZ_WS}
