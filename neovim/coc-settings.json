{
  "diagnostic.enable": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "snippets.ultisnips.enable": false,
  "languageserver": {
    "clangd": {
      "command": "clangd-7",
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    }
  }
}
