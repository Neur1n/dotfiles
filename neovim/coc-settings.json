{
  "diagnostic.enable": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "",
  "git.branchCharacter": "",
  "snippets.ultisnips.enable": true,
  "snippets.userSnippetsDirectory": "~/.config/nvim/configs/plugin_conf/coc-snippets",
  "python.jediEnabled": false,
  "languageserver": {
    "clangd": {
      "command": "clangd-7",
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    }
  }
}
